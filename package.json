{
  "name": "traffic-analysis-backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "start": "node dist/server.js",
    "build": "tsc && tsc-alias && node fix-imports.js",
    "dev": "nodemon --watch src --ext ts,json --exec \"npm run dev:exec\"",
    "dev:exec": "npm run build && node dist/server.js",
    "postinstall": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@types/node": "^22.0.0",
    "axios": "^1.6.8",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "mongoose": "^8.5.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.10",
    "nodemon": "^3.1.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.5.4"
  }
}
