{
  "name": "traffic-analysis-backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "start": "node dist/server.js",
    "build": "tsc && tsc-alias && node fix-imports.js",
    "dev": "nodemon --watch src --ext ts,json --exec \"npm run dev:exec\"",
    "dev:exec": "npm run build && node dist/server.js",
    "postinstall": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "helmet": "^7.2.0",
    "mongoose": "^8.18.0",
    "morgan": "^1.10.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.18.0",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.2"
  }
}
